        pass

    def onUnload(self):
        #put clean-up code here
        service = self.getParameter("Service Name")
        service_manager = self.session().service('ALServiceManager')
        if service_manager.isServiceRunning(service):
            service_manager.stopService(service)

    def onInput_onStart(self):
        service = self.getParameter("Service Name")
        service_manager = self.session().service('ALServiceManager')
        if not service_manager.isServiceRunning(service):
            service_manager.startService(service)
        self.onStopped()


    def onInput_onResults(self, p):
        import json
        j = json.loads(p)

        if len(j["results"]) == 0:
            return

        transcript = j["results"][0]["alternatives"][0]["transcript"].encode("utf-8")
        confidence = j["results"][0]["alternatives"][0]["confidence"]
        isFinal = j["results"][0]["final"]
        self.logger.info("transcript:" + transcript + "  confidence:" + str(confidence) + "  isFinal:" +  str(isFinal))

        if isFinal:
            self.onStopped(transcript)

    def onInput_onState(self, p):
        import json
        j = json.loads(p)

        if "state" in j and j["state"] == "listening":
            self.listening()


    def onInput_onStop(self):
        self.onUnload() #it is recommended to reuse the clean-up as the box is stopped
        self.onStopped() #activate the output of the box]]>
